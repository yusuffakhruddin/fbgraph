{
  "manifest_version": 2,

  "name": "FBGraphSearch",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },

  "icons" :{
    "64"  : "icon.png"
  },
    "content_scripts": [
      {
        "matches": ["http://*/*","https://*/*"],
        "js": ["myscript.js"]
      }
    ]
}
